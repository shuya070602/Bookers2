  <div class="container">
  <div class="row">
   <div class="col-xs-3">
    <%= render 'books/list', book: @book, user: @user %>
   </div>
   <div class="col-xs-9">
   <h3>Users</h3>
   <table class="table table-condensed">
   <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
   </thead>
  <% @users.each do |users| %>
   <tbody>
    <tr>
     <td>
      <%= attachment_image_tag users, :profile_image, :fill, 50, 50, format: 'jpeg', fallback: "no_image.jpg", size:"50x50" %>
     </td>
     <td>
     <%= users.name %>
     </td>
     <td>
     <%= link_to "Show", user_path(users.id) %>
     </td>
    </tr>
   <% end %>
   </tbody>
   </table>
   </div>
  </div>
  </div>